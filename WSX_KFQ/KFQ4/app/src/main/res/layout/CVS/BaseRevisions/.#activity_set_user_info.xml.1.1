<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/content_bg"
    android:orientation="vertical"
    tools:context="com.inetgoes.fangdd.activity.SetUserInfoActivity">

    <include
        android:id="@+id/set_layout_icon"
        layout="@layout/layout_set_icon" />

    <include
        android:id="@+id/set_layout_name"
        layout="@layout/layout_set_normal" />

    <include
        android:id="@+id/set_layout_phone"
        layout="@layout/layout_set_normal" />

    <include
        android:id="@+id/set_layout_mailbox"
        layout="@layout/layout_set_normal" />

    <include
        android:id="@+id/set_layout_auth"
        layout="@layout/layout_set_normal" />

</LinearLayout>
